    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="/">Pet Shoppe</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#textoNavbar" aria-controls="textoNavbar" aria-expanded="false" aria-label="Alterna navegação">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="textoNavbar">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                <a class="nav-link" href="/">Página Principal<span class="sr-only">(Página atual)</span></a>
                </li>
                <li class="nav-item">
                <a class="nav-link" href="/listaAnimais">Lista de Animais Disponíveis</a>
                </li>
                <li class="nav-item">
                <a class="nav-link" href="/listaBrinquedos">Lista de Brinquedos Disponíveis</a>
                </li>
                <li class="nav-item">
                <a class="nav-link" href="/listaHigiene">Lista de Produtos de Higiene Disponíveis</a>
                </li>
                <li class="nav-item active">
                <a class="nav-link" href="/cadastroCliente">Cadastre-se Agora!</a>
                </li>
                
            </ul>
            </div>
        </nav>
    </header>
    <form action="/listarCliente" method="POST">
        <p>Nome: </p>
        <input type="text" name="nome">
        <p>Sobrenome:</p>
        <input type="text" name="sobrenome">
        <p>E-mail: </p>
        <input type="email" name="email">
        <p>Senha: </p>
        <input type="password" name="senha">
        <p>Telefone: </p>
        <input type="tel" name="telefone">
        <br><br>
        <button type="submit">Cadastrar-se</button>
    </form>
</body>